
; BEGIN_SHADERTEST
; RUN: amdllpc -v %gfxip %s | FileCheck -check-prefix=SHADERTEST %s
; SHADERTEST-LABEL: {{^// LLPC}} SPIRV-to-LLVM translation results
; SHADERTEST: <4 x float> @_Z13quantizeToF16Dv4_f

; SHADERTEST-LABEL: {{^// LLPC}} pipeline patching results
; SHADERTEST: fptrunc float
; SHADERTEST: fpext half %{{[0-9]*}} to float
; SHADERTEST: call float @llvm.fabs.f32
; SHADERTEST: fcmp olt float
; SHADERTEST: fcmp one half
; SHADERTEST: and i1 %{{[0-9]*}}, %{{[0-9]*}}
; SHADERTEST: select i1
; SHADERTEST: fcmp uno float

; SHADERTEST : AMDLLPC SUCCESS
; END_SHADERTEST

